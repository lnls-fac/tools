           b3c9, mclcacp, b3c10, b3c11, b3c12, b3c13)
dispia   : line = (b1, d61, sd1, ch, cv, sd1, d17, qf1, d14, mon, &
           d12, sf1, qs, sf1, d23, qf2, d17, sd2, ch, sd2, d21, mon, d13, &
           b2, d46, sd3, cv, sd3, d17, qf3, d23, sf2, &
           ch, sf2, d12, mon, d14, qf4, d17, crhv, d43, mon, d13, &
           db3, qb3, b3bce)
dispca   : line = (b3bcs, qb3, db3, d73, qf4, d14, mon, d12, sf2, &
           ch, sf2, d23, qf3, d17, sd3, cv, sd3, d46, &
           b2, d34, sd2, ch, sd2, d17, qf2, d23, sf1, qs, sf1, d12, &
           mon, d14, qf1, d17, sd1, ch, cv, sd1, d48, mon, d13, &
           b1)
dispib   : line = (b1, d61, sd6, ch, cv, sd6, d17, qf1, d14, mon, &
           d12, sf4, qs, sf4, d23, qf2, d17, sd5, ch, sd5, d21, mon, d13, &
           b2, d46, sd4, cv, sd4, d17, qf3, d23, sf3, &
           ch, sf3, d12, mon, d14, qf4, d17, crhv, d43, mon, d13, &
           db3, qb3, b3bce)
dispcb   : line = (b3bcs, qb3, db3, d73, qf4, d14, mon, d12, sf3, &
           ch, sf3, d23, qf3, d17, sd4, cv, sd4, d46, &
           b2, d34, sd5, ch, sd5, d17, qf2, d23, sf4, qs, sf4, d12, &
           mon, d14, qf1, d17, sd6, ch, cv, sd6, d48, mon, d13, &
           b1)
insa     : line = (dia1, ampovu, ampvac, dia2, crhv, d16, sfa, &
           ch, cv, sfa, d12, mon, d14, qfa, d15, sda, qs, sda, d23, &
           qda, d17)
insb     : line = (dib1, ampivu, ampvac, dib2, qdb2, d15, crhv, &
           d16, sfb, ch, cv, sfb, d12, mon, d14, qfb, d15, sdb, &
           qs, sdb, d15, qdb1, d22)
hsupia   : line = (insa, mclcacp, dispia)
hsupca   : line = (dispca, mclcacp, -insa)
hsupib   : line = (insb, mclcacp, dispib)
hsupcb   : line = (dispcb, mclcacp, -insb)
supa     : line = (hsupia, hsupca)
supb     : line = (hsupib, hsupcb)
supab    : line = (hsupia, mc, mclcacp, hsupcb)
supba    : line = (hsupib, mc, mclcacp, hsupca)
anela    : line = (supa)
anelb    : line = (supb)
anelab   : line = (mia, supab, mib, supba)
supca    : line = (hsupca,mia, hsupia)
supcb    : line = (hsupcb,mib, hsupib)
sup4c    : line = (mia,hsupia,mc,supcb,mc,supcb,mc,supcb,mc,hsupca)
anel     : line = (5*sup4c)

! ring with initial misalignment and watch point for centroid observation
malin: malign,on_pass=0
w1      : watch,filename="%s.w1",mode="centroid"
anelw   : line=(malin,w1,anel)

! ring with with rf cavity and radiation damping, but no quantum excitation
! 5.320e-01 is the energy loss per turn
rfc     : rfca, volt=3.0e6, freq="c_mks 5.18396e2 / 864 *", phase="180 5.320e-1 3.0 / dasin -"
sr      : sreffects,qexcitation=0
anelrf  : line=(anel, sr,rfc)
